<view class='navBar' style='height:{{navBarHeight + statusBarHeight}}px;'>
    <view class='title' style='height:{{navBarHeight}}px;line-height:{{navBarHeight}}px'>{{header.title}}</view>

    <view class="navbarIcon" style="top:{{navBarHeight - statusBarHeight + 7}}px" >
        <mp-icon wx:if="{{header.isBack}}" class="iconBack" icon="back" size="{{13}}"></mp-icon>
        <mp-icon wx:if="{{header.isHome}}" class="iconHome" icon="home" size="{{25}}"></mp-icon>
    </view>
</view>